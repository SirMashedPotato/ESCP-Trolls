<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ReGrowth: Wasteland</li>
			<li>ReGrowth - Wasteland</li>
			<li>[HLX] ReGrowth - Wasteland</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>
			
				<!-- RG-W_Wasteland-->
			
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="RG-W_Wasteland"]/wildAnimals</xpath>
					<value>
						<MorrowRim_TrollBarren>0.02</MorrowRim_TrollBarren>
						<MorrowRim_TrollHill>0.005</MorrowRim_TrollHill>
					</value>			
				</li>

			</operations>
		</match>
	</Operation>

</Patch>

