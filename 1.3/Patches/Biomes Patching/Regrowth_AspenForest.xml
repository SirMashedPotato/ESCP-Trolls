<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ReGrowth - Aspen Forests</li>
			<li>ReGrowth: Aspen Forests</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>
			
				<!-- RG-W_Wasteland-->
			
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="RG-AF_AspenForest"]/wildAnimals</xpath>
					<value>
						<MorrowRim_TrollForest>0.07</MorrowRim_TrollForest>
						<MorrowRim_TrollHill>0.05</MorrowRim_TrollHill>
						<MorrowRim_TrollFrost>0.04</MorrowRim_TrollFrost>
						<MorrowRim_TrollUdyrfrykte>0.02</MorrowRim_TrollUdyrfrykte>
						<VAECaves_MorrowRim_TrollCave MayRequire="VanillaExpanded.VAECaves">0.05</VAECaves_MorrowRim_TrollCave>
					</value>			
				</li>

			</operations>
		</match>
	</Operation>

</Patch>

