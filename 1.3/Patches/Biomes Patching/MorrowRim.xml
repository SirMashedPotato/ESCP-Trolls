<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>MorrowRim</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>
			
				<!-- MorrowRim_Ashlands -->
			
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="MorrowRim_Ashlands"]/wildAnimals</xpath>
					<value>
						<MorrowRim_TrollFungal>0.15</MorrowRim_TrollFungal>
						<MorrowRim_TrollBarren>0.1</MorrowRim_TrollBarren>
						<MorrowRim_TrollHill>0.05</MorrowRim_TrollHill>
						<MorrowRim_TrollUdyrfrykte>0.02</MorrowRim_TrollUdyrfrykte>
						<VAECaves_MorrowRim_TrollCave MayRequire="VanillaExpanded.VAECaves">0.05</VAECaves_MorrowRim_TrollCave>
					</value>			
				</li>

			</operations>
		</match>
	</Operation>
	
