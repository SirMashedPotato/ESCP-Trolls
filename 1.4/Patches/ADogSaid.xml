<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>A Dog Said... Animal Prosthetics</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<!-- "Inserting animal defNames into surgery recipes == peglegs and healing" -->
					<xpath>Defs/RecipeDef[defName = "InstallPegLegAnimal" or "OldWoundsAnimalBase"]/recipeUsers</xpath>
					<value>
						<li>ESCP_TrollBarren</li>
						<li>ESCP_TrollForest</li>
						<li>ESCP_TrollPygmy</li>
						<li>ESCP_TrollSwamp</li>
						<li>ESCP_TrollFrost</li>
						<li>ESCP_TrollHill</li>
						<li>ESCP_TrollRiver</li>
						<li>ESCP_TrollGrahl</li>
						<li>ESCP_TrollUdyrfrykte</li>
						<li>ESCP_TrollFungal</li>
						<li>ESCP_TrollBoom</li>
						<li>ESCP_TrollCoastal</li>
						<li>ESCP_TrollCave</li>
						<li>ESCP_TrollAnima</li>
						<li MayRequire="Ludeon.RimWorld.Biotech">ESCP_TrollToxi</li>
						<li MayRequire="Ludeon.RimWorld.Biotech">ESCP_TrollHaemo</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<!-- "Inserting animal defNames into surgery recipes == simple prosthetics and organs" -->
					<xpath>Defs/RecipeDef[defName = "InstallNoseAnimalSimple" or "InstallBionicAnimalStomach" or "InstallBionicAnimalKidney" or "InstallBionicAnimalLiver" or "InstallBionicAnimalLung" or "InstallBionicAnimalHeart" or "InstallSpineAnimalSimple" or "InstallJawAnimalSimple" or "InstallBionicEyeAnimal" or "InstallEarAnimalSimple" or "InstallSimpleProstheticLegAnimal"]/recipeUsers</xpath>
					<value>
						<li>ESCP_TrollBarren</li>
						<li>ESCP_TrollForest</li>
						<li>ESCP_TrollPygmy</li>
						<li>ESCP_TrollSwamp</li>
						<li>ESCP_TrollFrost</li>
						<li>ESCP_TrollHill</li>
						<li>ESCP_TrollRiver</li>
						<li>ESCP_TrollGrahl</li>
						<li>ESCP_TrollUdyrfrykte</li>
						<li>ESCP_TrollFungal</li>
						<li>ESCP_TrollBoom</li>
						<li>ESCP_TrollCoastal</li>
						<li>ESCP_TrollCave</li>
						<li>ESCP_TrollAnima</li>
						<li MayRequire="Ludeon.RimWorld.Biotech">ESCP_TrollToxi</li>
						<li MayRequire="Ludeon.RimWorld.Biotech">ESCP_TrollHaemo</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<!-- "Inserting animal defNames into surgery recipes == bionics and powerclaw" -->
					<xpath>Defs/RecipeDef[defName = "InstallAnimalBrainStimulator" or "InstallSpineAnimalBionic" or "InstallJawAnimalBionic" or "InstallEarAnimalBionic" or "InstallPowerClawAnimal" or "InstallBionicLegAnimal"]/recipeUsers</xpath>
					<value>
						<li>ESCP_TrollBarren</li>
						<li>ESCP_TrollForest</li>
						<li>ESCP_TrollPygmy</li>
						<li>ESCP_TrollSwamp</li>
						<li>ESCP_TrollFrost</li>
						<li>ESCP_TrollHill</li>
						<li>ESCP_TrollRiver</li>
						<li>ESCP_TrollGrahl</li>
						<li>ESCP_TrollUdyrfrykte</li>
						<li>ESCP_TrollFungal</li>
						<li>ESCP_TrollBoom</li>
						<li>ESCP_TrollCoastal</li>
						<li>ESCP_TrollCave</li>
						<li>ESCP_TrollAnima</li>
						<li MayRequire="Ludeon.RimWorld.Biotech">ESCP_TrollToxi</li>
						<li MayRequire="Ludeon.RimWorld.Biotech">ESCP_TrollHaemo</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<!-- "Inserting animal defNames into surgery recipes == for prosthetic arms (if the animal has that bodypart only!)" -->
					<xpath>Defs/RecipeDef[defName = "InstallSimpleProstheticArmAnimal"]/recipeUsers</xpath>
					<value>
						<li>ESCP_TrollBarren</li>
						<li>ESCP_TrollForest</li>
						<li>ESCP_TrollPygmy</li>
						<li>ESCP_TrollSwamp</li>
						<li>ESCP_TrollFrost</li>
						<li>ESCP_TrollHill</li>
						<li>ESCP_TrollRiver</li>
						<li>ESCP_TrollGrahl</li>
						<li>ESCP_TrollUdyrfrykte</li>
						<li>ESCP_TrollFungal</li>
						<li>ESCP_TrollBoom</li>
						<li>ESCP_TrollCoastal</li>
						<li>ESCP_TrollCave</li>
						<li>ESCP_TrollAnima</li>
						<li MayRequire="Ludeon.RimWorld.Biotech">ESCP_TrollToxi</li>
						<li MayRequire="Ludeon.RimWorld.Biotech">ESCP_TrollHaemo</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<!-- "Inserting animal defNames into surgery recipes == for bionic arms (if the animal has that bodypart only!)" -->
					<xpath>Defs/RecipeDef[defName = "InstallBionicArmAnimal"]/recipeUsers</xpath>
					<value>
						<li>ESCP_TrollBarren</li>
						<li>ESCP_TrollForest</li>
						<li>ESCP_TrollPygmy</li>
						<li>ESCP_TrollSwamp</li>
						<li>ESCP_TrollFrost</li>
						<li>ESCP_TrollHill</li>
						<li>ESCP_TrollRiver</li>
						<li>ESCP_TrollGrahl</li>
						<li>ESCP_TrollUdyrfrykte</li>
						<li>ESCP_TrollFungal</li>
						<li>ESCP_TrollBoom</li>
						<li>ESCP_TrollCoastal</li>
						<li>ESCP_TrollCave</li>
						<li>ESCP_TrollAnima</li>
						<li MayRequire="Ludeon.RimWorld.Biotech">ESCP_TrollToxi</li>
						<li MayRequire="Ludeon.RimWorld.Biotech">ESCP_TrollHaemo</li>
